<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:horizontalGap="0px"
    android:keyWidth="10%p"
    android:keyHeight="@dimen/key_height"
    android:verticalGap="0px">
    <Row>
        <Key
            android:codes="-1"
            android:keyWidth="20%p"
            android:keyEdgeFlags="left"
            android:keyLabel="123" />
        <Key
            android:codes="42"
            android:keyWidth="20%p"
            android:keyLabel="*" />
        <Key
            android:codes="45"
            android:keyWidth="20%p"
            android:keyLabel="-" />
        <Key
            android:codes="38"
            android:keyWidth="20%p"
            android:keyLabel="&amp;" />

        <Key
            android:codes="-5"
            android:isRepeatable="true"
            android:keyWidth="20%p"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/sym_keyboard_delete" />
    </Row>

    <Row>
        <!--上一个界面-->
        <Key
            android:codes="-102"
            android:isRepeatable="true"
            android:keyWidth="20%p"
            android:keyEdgeFlags="left"
            android:keyIcon="@android:drawable/ic_media_previous" />
        <Key
            android:codes="94"
            android:keyWidth="20%p"
            android:keyLabel="^" />
        <Key
            android:codes="59"
            android:keyWidth="20%p"
            android:keyLabel=";" />
        <Key
            android:codes="37"
            android:keyWidth="20%p"
            android:keyLabel="%" />

        <!--到下一个界面英文界面-->
        <Key
            android:codes="-103"
            android:isRepeatable="true"
            android:keyWidth="20%p"
            android:keyEdgeFlags="right"
            android:keyIcon="@android:drawable/ic_media_next" />
    </Row>

    <Row android:rowEdgeFlags="bottom">
        <Key
            android:codes="40"
            android:keyWidth="20%p"
            android:keyEdgeFlags="left"
            android:keyLabel="(" />
        <Key
            android:codes="41"
            android:keyWidth="20%p"
            android:keyLabel=")" />

        <!--这个看起来是必须存在的，和Google的这块代码可能有关-->
        <Key
            android:codes="-2"
            android:keyWidth="0%p"
            android:keyLabel=" " />
        <!--
            android:codes: -101 is not a framework-defined key code but a key code that is
            privately defined in com.example.android.softkeyboard.LatinKeyboardView.
        -->
        <Key
            android:codes="-101"
            android:keyWidth="0%p"
            android:keyIcon="@drawable/sym_keyboard_language_switch" />
        <Key
            android:codes="92"
            android:keyWidth="20%p"
            android:keyLabel="\\" />
        <Key
            android:codes="124"
            android:keyWidth="20%p"
            android:keyLabel="|" />
        <Key
            android:codes="-3"
            android:keyWidth="20%p"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/sym_keyboard_done" />
    </Row>
</Keyboard>
