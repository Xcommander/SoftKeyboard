<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    android:horizontalGap="0px"
    android:keyWidth="10%p"
    android:keyHeight="@dimen/key_height"
    android:verticalGap="0px">

    <Row>
        <Key
            android:codes="-2"
            android:keyWidth="20%p"
            android:keyEdgeFlags="left"
            android:keyLabel="123" />
        <Key
            android:codes="115"
            android:keyWidth="20%p"
            android:keyLabel="s" />
        <Key
            android:codes="116"
            android:keyWidth="20%p"
            android:keyLabel="t" />
        <Key
            android:codes="117"
            android:keyWidth="20%p"
            android:keyLabel="u" />

        <Key
            android:codes="-5"
            android:isRepeatable="true"
            android:keyWidth="20%p"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/sym_keyboard_delete" />
    </Row>

    <Row>
        <!--上一个界面-->
        <Key
            android:codes="-102"
            android:isRepeatable="true"
            android:keyWidth="20%p"
            android:keyEdgeFlags="left"
            android:keyIcon="@android:drawable/ic_media_previous" />
        <Key
            android:codes="118"
            android:keyWidth="20%p"
            android:keyLabel="v" />
        <Key
            android:codes="119"
            android:keyWidth="20%p"
            android:keyLabel="w" />
        <Key
            android:codes="120"
            android:keyWidth="20%p"
            android:keyLabel="x" />
        <!--到下一个界面英文界面-->
        <Key
            android:codes="-103"
            android:isRepeatable="true"
            android:keyWidth="20%p"
            android:keyEdgeFlags="right"
            android:keyIcon="@android:drawable/ic_media_next" />
    </Row>

    <Row android:rowEdgeFlags="bottom">
        <Key
            android:codes="-1"
            android:isModifier="true"
            android:isSticky="true"
            android:keyWidth="20%p"
            android:keyEdgeFlags="left"
            android:keyIcon="@drawable/sym_keyboard_shift" />

        <Key
            android:codes="-101"
            android:keyWidth="0%p"
            android:keyIcon="@drawable/sym_keyboard_language_switch" />
        <Key
            android:codes="121"
            android:keyWidth="20%p"
            android:keyLabel="y" />
        <Key
            android:codes="122"
            android:keyWidth="20%p"
            android:keyLabel="z" />
        <Key
            android:codes="-100"
            android:keyWidth="20%p" />
        <Key
            android:codes="-3"
            android:keyWidth="20%p"
            android:keyEdgeFlags="right"
            android:keyIcon="@drawable/sym_keyboard_done" />
    </Row>
</Keyboard>
